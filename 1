local GUI = Instance.new("ScreenGui")
local Backgroundgui = Instance.new("Frame")
local Bouton = Instance.new("TextButton")
local Title = Instance.new("TextLabel")
local Credits = Instance.new("TextBox")
local Bouton2 = Instance.new("TextButton")
local Bouton3 = Instance.new("TextButton")

--Properties:
GUI.Name = "GUI_SHEET"
GUI.Parent = game:GetService("Players").LocalPlayer.PlayerGui.ChatGui

Backgroundgui.Name = "Backgroundgui"
Backgroundgui.Parent = GUI
Backgroundgui.Active = true
Backgroundgui.BackgroundColor3 = Color3.new(0.129412, 0.129412, 0.129412)
Backgroundgui.BorderSizePixel = 0
Backgroundgui.Position = UDim2.new(0.144435674, 0, 0.224815726, 0)
Backgroundgui.Selectable = trueK
Backgroundgui.Size = UDim2.new(0, 300, 0, 400)
Backgroundgui.Style = Enum.FrameStyle.RobloxRound

Bouton.Name = "Bouton"
Bouton.Parent = Backgroundgui
Bouton.BackgroundColor3 = Color3.new(0.129412, 0.129412, 0.129412)
Bouton.BorderSizePixel = 2
Bouton.Position = UDim2.new(0.146666661, 0, 0.120907381, 0)
Bouton.Size = UDim2.new(0, 200, 0, 50)
Bouton.Modal = true
Bouton.Font = Enum.Font.SourceSans
Bouton.Text = "anti hostage"
Bouton.TextColor3 = Color3.new(255, 1, 1)
Bouton.TextSize = 14
Bouton.MouseButton1Down:connect(function() 
local newcclosure = newcclosure or function(f)
return f
end
loadstring(game:HttpGet("https://raw.githubusercontent.com/kirillka77734/roblox/main/2", true))()
end)
Title.Name = "Title"
Title.Parent = Backgroundgui
Title.BackgroundColor3 = Color3.new(0, 0, 0)
Title.Position = UDim2.new(-0.0245996583, 0, -0.0692985579, 0)
Title.Size = UDim2.new(0, 298, 0, 23)
Title.Font = Enum.Font.SourceSans
